<commands>
    <commandSet>
        <id>shell</id>
        <enabled>true</enabled>
        <autoRegisterPriority>20</autoRegisterPriority>
        <commands>
            <command>
                <type>org.sonatype.gshell.commands.shell.AskCommand</type>
            </command>
            <command>
                <type>org.sonatype.gshell.commands.shell.SleepCommand</type>
            </command>
            <command>
                <type>org.sonatype.gshell.commands.shell.ClearCommand</type>
            </command>
            <command>
                <type>org.sonatype.gshell.commands.shell.DateCommand</type>
            </command>
            <command>
                <type>org.sonatype.gshell.commands.shell.ExecuteCommand</type>
            </command>
            <command>
                <type>org.sonatype.gshell.commands.shell.JavaCommand</type>
            </command>
        </commands>
    </commandSet>
</commands>
